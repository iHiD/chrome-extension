{
  "manifest_version": 2,

  "name": "Meducation",
  "description": "Share and comment on resources on Meducation.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "https://www.meducation.net/",
    "http://localhost:3000/"
  ],

  "content_scripts": [
    {
      "matches": ["https://soundcloud.com/*"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
    
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bookmark.html"
  }
}
